    "async def check_openai_quota():\n",
    "    \"\"\"\n",
    "    Check OpenAI API quota and usage status.\n",
    "    \n",
    "    Returns:\n",
    "        dict: Status information including quota availability\n",
    "    \"\"\"\n",
    "    api_key = os.getenv(\"OPENAI_API_KEY\")\n",
    "    if not api_key:\n",
    "        return {\"status\": \"error\", \"message\": \"No API key found\"}\n",
    "    \n",
    "    headers = {\n",
    "        \"Authorization\": f\"Bearer {api_key}\",\n",
    "        \"Content-Type\": \"application/json\"\n",
    "    }\n",
    "    \n",
    "    try:\n",
    "        async with aiohttp.ClientSession() as session:\n",
    "            # Test with a minimal API call to check quota\n",
    "            test_payload = {\n",
    "                \"model\": \"gpt-4o-mini\",\n",
    "                \"messages\": [{\"role\": \"user\", \"content\": \"test\"}],\n",
    "                \"max_tokens\": 1\n",
    "            }\n",
    "            \n",
    "            async with session.post(\n",
    "                \"https://api.openai.com/v1/chat/completions\",\n",
    "                headers=headers,\n",
    "                json=test_payload,\n",
    "                timeout=aiohttp.ClientTimeout(total=10)\n",
    "            ) as response:\n",
    "                \n",
    "                if response.status == 200:\n",
    "                    return {\n",
    "                        \"status\": \"success\", \n",
    "                        \"message\": \"âœ… OpenAI API quota available\",\n",
    "                        \"quota_available\": True\n",
    "                    }\n",
    "                elif response.status == 429:\n",
    "                    error_data = await response.json()\n",
    "                    return {\n",
    "                        \"status\": \"quota_exceeded\",\n",
    "                        \"message\": \"âŒ OpenAI API quota exceeded\",\n",
    "                        \"error\": error_data.get(\"error\", {}).get(\"message\", \"Unknown quota error\"),\n",
    "                        \"quota_available\": False\n",
    "                    }\n",
    "                else:\n",
    "                    error_data = await response.json()\n",
    "                    return {\n",
    "                        \"status\": \"error\",\n",
    "                        \"message\": f\"âŒ OpenAI API error (Status: {response.status})\",\n",
    "                        \"error\": error_data.get(\"error\", {}).get(\"message\", \"Unknown error\"),\n",
    "                        \"quota_available\": False\n",
    "                    }\n",
    "                    \n",
    "    except asyncio.TimeoutError:\n",
    "        return {\n",
    "            \"status\": \"timeout\",\n",
    "            \"message\": \"â±ï¸ OpenAI API timeout - network issues\",\n",
    "            \"quota_available\": False\n",
    "        }\n",
    "    except Exception as e:\n",
    "        return {\n",
    "            \"status\": \"error\",\n",
    "            \"message\": f\"âŒ OpenAI API check failed: {str(e)}\",\n",
    "            \"quota_available\": False\n",
    "        }\n",
    "\n",
    "print(\"ðŸ” OpenAI quota check function defined\")"
   ]
  },
  {
