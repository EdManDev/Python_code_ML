    "async def check_elevenlabs_quota():\n",
    "    \"\"\"\n",
    "    Check ElevenLabs API quota and usage status.\n",
    "    \n",
    "    Returns:\n",
    "        dict: Status information including quota availability\n",
    "    \"\"\"\n",
    "    api_key = os.getenv(\"ELEVEN_API_KEY\")\n",
    "    if not api_key:\n",
    "        return {\"status\": \"error\", \"message\": \"No API key found\"}\n",
    "    \n",
    "    headers = {\n",
    "        \"xi-api-key\": api_key,\n",
    "        \"Content-Type\": \"application/json\"\n",
    "    }\n",
    "    \n",
    "    try:\n",
    "        async with aiohttp.ClientSession() as session:\n",
    "            # Check user info and quota\n",
    "            async with session.get(\n",
    "                \"https://api.elevenlabs.io/v1/user\",\n",
    "                headers=headers,\n",
    "                timeout=aiohttp.ClientTimeout(total=10)\n",
    "            ) as response:\n",
    "                \n",
    "                if response.status == 200:\n",
    "                    user_data = await response.json()\n",
    "                    subscription = user_data.get(\"subscription\", {})\n",
    "                    character_count = subscription.get(\"character_count\", 0)\n",
    "                    character_limit = subscription.get(\"character_limit\", 0)\n",
    "                    \n",
    "                    if character_limit > 0:\n",
    "                        usage_percent = (character_count / character_limit) * 100\n",
    "                        remaining = character_limit - character_count\n",
    "                        \n",
    "                        if remaining <= 0:\n",
    "                            return {\n",
    "                                \"status\": \"quota_exceeded\",\n",
    "                                \"message\": f\"âŒ ElevenLabs quota exceeded ({character_count}/{character_limit} characters)\",\n",
    "                                \"quota_available\": False,\n",
    "                                \"usage_percent\": usage_percent\n",
    "                            }\n",
    "                        elif usage_percent > 90:\n",
    "                            return {\n",
    "                                \"status\": \"warning\",\n",
    "                                \"message\": f\"âš ï¸ ElevenLabs quota low ({remaining} characters remaining)\",\n",
    "                                \"quota_available\": True,\n",
    "                                \"usage_percent\": usage_percent\n",
    "                            }\n",
    "                        else:\n",
    "                            return {\n",
    "                                \"status\": \"success\",\n",
    "                                \"message\": f\"âœ… ElevenLabs quota available ({remaining} characters remaining)\",\n",
    "                                \"quota_available\": True,\n",
    "                                \"usage_percent\": usage_percent\n",
    "                            }\n",
    "                    else:\n",
    "                        return {\n",
    "                            \"status\": \"success\",\n",
    "                            \"message\": \"âœ… ElevenLabs API accessible\",\n",
    "                            \"quota_available\": True\n",
    "                        }\n",
    "                        \n",
    "                elif response.status == 401:\n",
    "                    return {\n",
    "                        \"status\": \"auth_error\",\n",
    "                        \"message\": \"âŒ ElevenLabs API key invalid\",\n",
    "                        \"quota_available\": False\n",
    "                    }\n",
    "                elif response.status == 429:\n",
    "                    return {\n",
    "                        \"status\": \"quota_exceeded\",\n",
    "                        \"message\": \"âŒ ElevenLabs API quota exceeded\",\n",
    "                        \"quota_available\": False\n",
    "                    }\n",
    "                else:\n",
    "                    return {\n",
    "                        \"status\": \"error\",\n",
    "                        \"message\": f\"âŒ ElevenLabs API error (Status: {response.status})\",\n",
    "                        \"quota_available\": False\n",
    "                    }\n",
    "                    \n",
    "    except asyncio.TimeoutError:\n",
    "        return {\n",
    "            \"status\": \"timeout\",\n",
    "            \"message\": \"â±ï¸ ElevenLabs API timeout - network issues\",\n",
    "            \"quota_available\": False\n",
    "        }\n",
    "    except Exception as e:\n",
    "        return {\n",
    "            \"status\": \"error\",\n",
    "            \"message\": f\"âŒ ElevenLabs API check failed: {str(e)}\",\n",
    "            \"quota_available\": False\n",
    "        }\n",
    "\n",
    "print(\"ðŸ” ElevenLabs quota check function defined\")"
   ]
  },
  {
   "cell_type": "code",
