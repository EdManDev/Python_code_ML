   "source": [
    "async def perform_api_health_check():\n",
    "    \"\"\"\n",
    "    Perform comprehensive API health check for all services.\n",
    "    \n",
    "    Returns:\n",
    "        dict: Overall health status and individual service results\n",
    "    \"\"\"\n",
    "    print(\"ðŸ” Checking API health and quotas...\")\n",
    "    print(f\"â° Time: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    # Check both APIs concurrently for faster results\n",
    "    openai_result, elevenlabs_result = await asyncio.gather(\n",
    "        check_openai_quota(),\n",
    "        check_elevenlabs_quota(),\n",
    "        return_exceptions=True\n",
    "    )\n",
    "    \n",
    "    # Handle potential exceptions\n",
    "    if isinstance(openai_result, Exception):\n",
    "        openai_result = {\n",
    "            \"status\": \"error\",\n",
    "            \"message\": f\"âŒ OpenAI check failed: {str(openai_result)}\",\n",
    "            \"quota_available\": False\n",
    "        }\n",
    "    \n",
    "    if isinstance(elevenlabs_result, Exception):\n",
    "        elevenlabs_result = {\n",
    "            \"status\": \"error\", \n",
    "            \"message\": f\"âŒ ElevenLabs check failed: {str(elevenlabs_result)}\",\n",
    "            \"quota_available\": False\n",
    "        }\n",
    "    \n",
    "    # Display results\n",
    "    print(f\"OpenAI Status: {openai_result['message']}\")\n",
    "    print(f\"ElevenLabs Status: {elevenlabs_result['message']}\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    # Determine overall status\n",
    "    all_available = (\n",
    "        openai_result.get(\"quota_available\", False) and \n",
    "        elevenlabs_result.get(\"quota_available\", False)\n",
    "    )\n",
    "    \n",
    "    if not all_available:\n",
    "        print(\"âŒ CRITICAL: One or more APIs have quota issues!\")\n",
    "        print(\"\\nðŸ”§ SOLUTIONS:\")\n",
    "        \n",
    "        if not openai_result.get(\"quota_available\", False):\n",
    "            print(\"   â€¢ OpenAI Issue:\")\n",
    "            print(\"     - Visit: https://platform.openai.com/account/billing\")\n",
    "            print(\"     - Add credits to your account\")\n",
    "            print(\"     - Check usage limits\")\n",
    "            \n",
    "        if not elevenlabs_result.get(\"quota_available\", False):\n",
    "            print(\"   â€¢ ElevenLabs Issue:\")\n",
    "            print(\"     - Visit: https://elevenlabs.io/subscription\")\n",
    "            print(\"     - Check your subscription status\")\n",
    "            print(\"     - Upgrade plan if needed\")\n",
    "        \n",
    "        print(\"\\nâš ï¸  Agent will likely fail during operation!\")\n",
    "        return {\n",
    "            \"overall_status\": \"failed\",\n",
    "            \"can_proceed\": False,\n",
    "            \"openai\": openai_result,\n",
    "            \"elevenlabs\": elevenlabs_result\n",
    "        }\n",
    "    else:\n",
    "        print(\"âœ… All APIs are healthy and ready!\")\n",
    "        return {\n",
    "            \"overall_status\": \"healthy\",\n",
    "            \"can_proceed\": True,\n",
    "            \"openai\": openai_result,\n",
    "            \"elevenlabs\": elevenlabs_result\n",
    "        }\n",
    "\n",
    "print(\"ðŸ” Comprehensive health check function defined\")"
   ]
